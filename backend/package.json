{
  "name": "@y-celestial/pollux-service",
  "version": "1.1.3",
  "author": "Yue Chang",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "lint:es": "eslint -c ./eslint.json src",
    "fix:es": "npm run lint:es -- --fix",
    "lint:format": "prettier --config ./prettierrc.json --list-different \"./**/*.ts\" --ignore-path .gitignore",
    "fix:format": "npm run lint:format -- --write",
    "lint": "run-p lint:*",
    "fix": "run-s fix:*",
    "clean": "del-cli dist lib -f",
    "compile": "ttsc",
    "test": "jest -c jest.config.js",
    "test:coverage": "npm run test -- --coverage --verbose",
    "build": "webpack --config webpack.config.js",
    "pre:commit": "run-s lint test:coverage build",
    "pre:dev": "run-s fix test:coverage",
    "pre:deploy": "run-s clean build",
    "post:publish": "run-s clean compile && npm publish"
  },
  "license": "ISC",
  "dependencies": {
    "@y-celestial/service": "^3.0.7",
    "axios": "^0.26.1",
    "date-fns": "^2.28.0",
    "inversify": "^6.0.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.3.1",
    "typeorm": "^0.3.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/multer": "^1.4.7",
    "@types/pg": "^8.6.5",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "del-cli": "^4.0.1",
    "eslint": "^8.11.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^27.3.1",
    "multer": "^1.4.5-lts.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "ttypescript": "^1.5.13",
    "typescript": "^4.6.2",
    "typescript-transform-paths": "^3.3.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "zip-webpack-plugin": "^4.0.1"
  },
  "files": [
    "lib/**/*"
  ]
}
