{
  "name": "pollux-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env BROWSER=none PORT=3000 react-scripts start",
    "build": "cross-env BUILD_PATH='./dist' react-scripts build",
    "test": "react-scripts test --coverage",
    "test:nowatch": "npm run test -- --watchAll=false",
    "eject": "react-scripts eject",
    "lint:es": "eslint -c ./eslint.json src",
    "fix:es": "npm run lint:es -- --fix",
    "lint:format": "prettier --list-different \"{src,public}/**/*.{tsx,ts,js,html,scss,json}\"",
    "fix:format": "npm run lint:format -- --write",
    "lint": "run-s lint:*",
    "fix": "run-s fix:*",
    "clean": "del-cli dist -f",
    "pre:commit": "npm run lint",
    "pre:deploy": "run-s clean build"
  },
  "dependencies": {
    "@editorjs/editorjs": "^2.24.1",
    "@editorjs/table": "^2.0.2",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.0",
    "@mui/material": "^5.8.2",
    "@reduxjs/toolkit": "^1.8.2",
    "amazon-cognito-identity-js": "^5.2.9",
    "axios": "^0.26.1",
    "better-react-mathjax": "^2.0.1",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "react": "^18.1.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.1.0",
    "react-editor-js": "^2.0.6",
    "react-hook-form": "^7.31.3",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.38",
    "@types/react": "^18.0.10",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@y-celestial/pollux-service": "^1.0.12",
    "cross-env": "^7.0.3",
    "del-cli": "^4.0.1",
    "eslint": "^8.16.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.0",
    "http-proxy-middleware": "^2.0.4",
    "jest-watch-typeahead": "^0.6.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "react-scripts": "5.0.0",
    "sass": "^1.52.1",
    "typescript": "^4.7.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
