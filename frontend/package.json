{
  "name": "pollux-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env BROWSER=none PORT=3000 react-scripts start",
    "build": "cross-env BUILD_PATH='./dist' react-scripts build",
    "test": "react-scripts test --coverage",
    "test:nowatch": "npm run test -- --watchAll=false",
    "eject": "react-scripts eject",
    "lint:es": "eslint -c ./eslint.json src",
    "fix:es": "npm run lint:es -- --fix",
    "lint:format": "prettier --list-different \"{src,public}/**/*.{tsx,ts,js,html,scss,json}\"",
    "fix:format": "npm run lint:format -- --write",
    "lint": "run-s lint:*",
    "fix": "run-s fix:*",
    "clean": "del-cli dist -f",
    "pre:commit": "npm run lint",
    "pre:deploy": "run-s clean build"
  },
  "dependencies": {
    "@mui/base": "^5.0.0-alpha.101",
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.37.0",
    "react-router-dom": "^6.4.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.59",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "autoprefixer": "^10.4.12",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "eslint": "^8.23.1",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.8",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "react-scripts": "5.0.1",
    "sass": "^1.55.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "^18.0.0",
    "npm": "^8.0.0"
  }
}
